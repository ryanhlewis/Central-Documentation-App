<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/login"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.centraldocs.centraldocs.ui.home.LoginFragment">


    <com.google.android.material.button.MaterialButton
        android:id="@+id/button2"
        android:layout_width="356dp"
        android:layout_height="72dp"
        android:layout_marginStart="109dp"
        android:layout_marginTop="508dp"
        android:layout_marginEnd="110dp"
        android:backgroundTint="#9BFFF7"
        android:contentDescription="Login Using Github"
        android:scaleType="fitEnd"
        android:src="@drawable/ic_baseline_fast_forward_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.Material3.Chip"
        app:tint="#0E0E0E" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button"
        android:layout_width="362dp"
        android:layout_height="142dp"
        android:layout_marginStart="109dp"
        android:layout_marginTop="352dp"
        android:layout_marginEnd="110dp"
        android:contentDescription="Login Using Github"
        android:scaleType="fitEnd"
        app:backgroundTint="#70F67D"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.Material3.Chip" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="229dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="384dp"
        android:layout_marginEnd="3dp"
        android:fontFamily="@font/fredoka_one"
        android:text="Login with Github"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="?attr/colorSecondaryVariant"
        android:translationZ="10dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/button"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="250dp"
        android:layout_height="38dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="524dp"
        android:layout_marginEnd="5dp"
        android:fontFamily="@font/fredoka_one"
        android:text="Skip"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="?attr/colorSecondaryVariant"
        android:translationZ="10dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="@+id/button2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="307dp"
        android:layout_height="48dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="77dp"
        android:text="Peer-edited, open, and understandable documentation for everyone."
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="374dp"
        android:layout_height="36dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="69dp"
        android:fontFamily="@font/fredoka_one"
        android:text="Central Documentation"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="123dp"
        android:layout_height="117dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="156dp"
        android:src="@drawable/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="107dp"
        android:layout_height="113dp"
        android:layout_marginStart="-110dp"
        android:layout_marginTop="368dp"
        android:src="@drawable/github"
        android:translationZ="10dp"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="76dp"
        android:layout_height="56dp"
        android:layout_marginStart="-80dp"
        android:layout_marginTop="516dp"
        android:src="@drawable/ic_baseline_fast_forward_24"
        android:translationZ="10dp"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toTopOf="parent"
        app:tint="?attr/colorSecondaryVariant" />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>